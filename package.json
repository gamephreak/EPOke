{
  "name": "epoke",
  "version": "0.0.1",
  "description": "Augmentation tooling for competitive Pokémon battling",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkmn-cc/EPOke.git"
  },
  "keywords": [
    "competitive",
    "Pokémon",
    "tools",
    "statistics"
  ],
  "author": "Kirk Scheibelhut",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^13.7.7",
    "ps": "file:ps",
    "@smogon/calc": "^0.0.4"
    "@smogon/stats": "file:../Pokemon-Showdown-Stats/stats"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "gts": "^1.1.2",
    "jest": "^25.1.0",
    "subpkg": "^0.0.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "check": "subpkg run check && gts check",
    "clean": "subpkg run clean && gts clean",
    "compile": "subpkg run compile && tsc -p .",
    "fix": "subpkg run fix && gts fix",
    "test": "subpkg run test && jest",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "postinstall": "subpkg install"
  },
  "subPackages": [
    "ps"
  ]
}
