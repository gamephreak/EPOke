    "@pkmn.cc/data": "file:../pkmn/data"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "gts": "^1.1.0",
    "jest": "^24.9.0",
    "subpkg": "^0.0.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  },
  "scripts": {
