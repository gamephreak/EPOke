{
  "name": "epoke",
  "version": "0.0.1",
  "description": "Augmentation tooling for competitive Pokémon battling",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkmn-cc/EPOke.git"
  },
  "keywords": [
    "competitive",
    "Pokémon",
    "tools",
    "statistics"
  ],
